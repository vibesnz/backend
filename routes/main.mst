  margin-top: 1rem !important;
  margin-bottom: 0;
}
.prompt-input {
  width: 100%;
  margin-bottom: 10px;
  font-family: "comic sans ms" !important;
  border-radius: 4px;
  border-width:1px;
  padding: 3px 8px;
}
.prompt-textarea {
  font-family: "comic sans ms" !important;
  border-radius: 4px;
  padding: 3px 8px;
}
.prompt-add-button {
  font-family: "comic sans ms" !important;
  margin-top: 10px;
  padding: 4px 20px;
  border-width: 1px;
  border-radius: 4px
}
.prompt-button {
  font-family: "comic sans ms" !important;
  margin-top: 10px;
  padding: 4px 20px;
  border-width: 1px;
  border-radius: 4px
}
#prompt-template {
  display: none;
}
</style>
<script>
function repeat(str) {
  return [
    str + "",
    str + ".",
    str + "..",
    str + "...",
  ]
}
function startLoading() {
  document.getElementById('loading').style.display = 'flex'
  var loadingCopies = [
    ...repeat("Training model"),
    ...repeat("Adjusting the parameters"),
    ...repeat("Generating HTML"),
    ...repeat("Loading"),
    ...repeat("Counting sheep"),
    ...repeat("Parsing HTML"),
    ...repeat("Getting HTML to run"),
    ...repeat("Consulting StackOverflow"),
    ...repeat("Debugging"),
    ...repeat("Hiring a consultant"),
    ...repeat("Giving a TED talk"),
    ...repeat("Making dinner"),
    ...repeat("Getting groceries"),
    ...repeat("Driving to the supermarket"),
  ]
  var counter = 0
  setInterval(function() {
    var copy = loadingCopies[counter % loadingCopies.length]
    document.getElementById('loading-text').innerHTML = copy
    counter++
  }, 300)
}
</script>
